-- New Steal A Brainrot

local Fluent = loadstring(game:HttpGet("https://raw.githubusercontent.com/discoart/FluentPlus/refs/heads/main/Beta.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Workspace = game:GetService("Workspace")

local Window = Fluent:CreateWindow({
    Title = "Nevox " .. 1.27,
    SubTitle = "by Madness.idk",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true,
    Theme = "Arctic",
    MinimizeKey = Enum.KeyCode.LeftControl
})

local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "building-2" }),
    Misc = Window:AddTab({ Title = "Misc", Icon = "wand" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" }),
    Credits = Window:AddTab({ Title = "Credits", Icon = "award" })
}

local Options = Fluent.Options

-- Speed stuff, etc (same as before) --

local desiredSpeed = 70
local speedBypassConnection = nil

for _, connection in ipairs(getconnections(RunService.RenderStepped)) do
    if connection.Function and islclosure(connection.Function) then
        local info = debug.getinfo(connection.Function)
        if info and info.source and string.find(info.source, "SpeedController") then
            pcall(function() connection:Disable() end)
            Fluent:Notify({
                Title = "Anti-Cheat Patched",
                Content = "Disabled anti-cheat.",
                Duration = 5
            })
        end
    end
end

Tabs.Main:AddToggle("SpeedBypass", {
    Title = "Speed Bypass",
    Description = "Go faster yo",
    Default = false
}):OnChanged(function(state)
    if state then
        if speedBypassConnection then speedBypassConnection:Disconnect() end
        speedBypassConnection = RunService.Heartbeat:Connect(function()
            local char = LocalPlayer.Character
            if char then
                local hum = char:FindFirstChildOfClass("Humanoid")
                if hum then hum.WalkSpeed = desiredSpeed end
            end
        end)
    else
        if speedBypassConnection then speedBypassConnection:Disconnect() end
        speedBypassConnection = nil
    end
end)

-- === BASE TIME ESP ===
local baseTimeESPEnabled = false
local baseBillboards = {}

local function createBaseTimeBillboard(plotBlock)
    if baseBillboards[plotBlock] then return end

    local billboard = Instance.new("BillboardGui")
    billboard.Name = "NevoxBaseTimeESP"
    billboard.Size = UDim2.new(0, 130, 0, 25)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true
    billboard.Adornee = plotBlock.Main
    billboard.Parent = plotBlock.Main

    local timeLabel = Instance.new("TextLabel")
    timeLabel.Size = UDim2.new(1, 0, 1, 0)
    timeLabel.BackgroundTransparency = 1
    timeLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
    timeLabel.TextStrokeTransparency = 0
    timeLabel.TextStrokeColor3 = Color3.new(0,0,0)
    timeLabel.Font = Enum.Font.SourceSansBold
    timeLabel.TextScaled = true
    timeLabel.Parent = billboard

    baseBillboards[plotBlock] = { Billboard = billboard, TimeLabel = timeLabel }
end

local function removeBaseTimeBillboard(plotBlock)
    if baseBillboards[plotBlock] then
        baseBillboards[plotBlock].Billboard:Destroy()
        baseBillboards[plotBlock] = nil
    end
end


Tabs.Main:AddToggle("BaseTimeESP", {
    Title = "ESP Base Remaining Time",
    Description = "Show RemainingTime text over all plots",
    Default = false
}):OnChanged(function(state)
    baseTimeESPEnabled = state

    if not baseTimeESPEnabled then
        for plotBlock, _ in pairs(baseBillboards) do
            removeBaseTimeBillboard(plotBlock)
        end
    end
end)

RunService.Heartbeat:Connect(function()
    if baseTimeESPEnabled then
        local plotsFolder = Workspace:FindFirstChild("Plots")
        if plotsFolder then
            for _, plot in pairs(plotsFolder:GetChildren()) do
                local purchases = plot:FindFirstChild("Purchases")
                if purchases then
                    for _, plotBlock in pairs(purchases:GetChildren()) do
                        local main = plotBlock:FindFirstChild("Main")
                        if main and main:FindFirstChild("BillboardGui") and main.BillboardGui:FindFirstChild("RemainingTime") then
                            if not baseBillboards[plotBlock] then
                                createBaseTimeBillboard(plotBlock)
                            end
                            local timeText = main.BillboardGui.RemainingTime.Text
                            local timeLabel = baseBillboards[plotBlock].TimeLabel
                            timeLabel.Text = timeText or ""
                        else
                            if baseBillboards[plotBlock] then
                                removeBaseTimeBillboard(plotBlock)
                            end
                        end
                    end
                end
            end
        end
    end
end)

-- === BRAINROT TARGET ESP ===
local brainrotTargetESPEnabled = false
local brainrotTargets = {}

local expensiveBrainrots = {
    ["Odin Din Din Dun"] = true,
    ["Cocofanto Elefanto"] = true,
    ["Tralalero Tralala"] = true,
    ["La Vacca Saturno Saturnita"] = true,
    ["Graipuss Medussi"] = true,
    ["Los Tralaleritos"] = true,
    ["La Grande Combinasion"] = true,
    ["Girafa Celestre"] = true,
    ["Gattatino Nyanino"] = true,
    ["Trenostruzzo Turbo 3000"] = true,
    ["Unclito Samito"] = true,
    ["Matteo"] = true,
    ["Graipuss Medussi"] = true,
    ["Sammyni Spyderini"] = true,
    ["Garama and Madundung"] = true,
    ["Kings Coleslaw"] = true,
    ["Orcalero Orcala"] = true,
    ["Tigroline Frutonni"] = true,
    ["tortuguini dragonfrutini"] = true,
    ["Developini Braziliaspidini"] = true
}

local function getPrimaryPart(model)
    return model.PrimaryPart or model:FindFirstChild("HumanoidRootPart") or model:FindFirstChildWhichIsA("BasePart")
end

local function createTargetSymbol(model)
    if brainrotTargets[model] then return end
    local part = getPrimaryPart(model)
    if not part then return end

    local billboard = Instance.new("BillboardGui")
    billboard.Name = "NevoxBrainrotTarget"
    billboard.Size = UDim2.new(0, 50, 0, 50)
    billboard.StudsOffset = Vector3.new(0, 3, 0)
    billboard.AlwaysOnTop = true
    billboard.Adornee = part
    billboard.Parent = part

    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 1, 0)
    label.BackgroundTransparency = 1
    label.Text = "ðŸŽ¯"
    label.TextColor3 = Color3.fromRGB(255, 0, 0)
    label.Font = Enum.Font.SourceSansBold
    label.TextScaled = true
    label.Parent = billboard

    brainrotTargets[model] = billboard
end

local function removeTargetSymbol(model)
    if brainrotTargets[model] then
        brainrotTargets[model]:Destroy()
        brainrotTargets[model] = nil
    end
end

Tabs.Main:AddToggle("BrainrotTargetESP", {
    Title = "ESP Brainrots",
    Description = "Shows a red target symbol above expensive Brainrots",
    Default = false
}):OnChanged(function(state)
    brainrotTargetESPEnabled = state
    if not brainrotTargetESPEnabled then
        for model, gui in pairs(brainrotTargets) do
            gui:Destroy()
        end
        brainrotTargets = {}
    end
end)

RunService.Heartbeat:Connect(function()
    if not brainrotTargetESPEnabled then return end

    local char = LocalPlayer.Character
    local hrp = char and char:FindFirstChild("HumanoidRootPart")
    if not hrp then return end

    -- Clean up removed models
    for model, _ in pairs(brainrotTargets) do
        if not model.Parent then
            removeTargetSymbol(model)
        end
    end

    -- Search workspace for expensive brainrots by name
    for _, model in pairs(Workspace:GetChildren()) do
        if model:IsA("Model") and expensiveBrainrots[model.Name] then
            if not brainrotTargets[model] then
                createTargetSymbol(model)
            end

            -- Hide target if player is close (within 50 studs)
            local part = getPrimaryPart(model)
            if part then
                local dist = (hrp.Position - part.Position).Magnitude
                if dist <= 50 then
                    if brainrotTargets[model] then
                        brainrotTargets[model].Enabled = false
                    end
                else
                    if brainrotTargets[model] then
                        brainrotTargets[model].Enabled = true
                    end
                end
            end
        end
    end
end)




local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer

local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local hrp = character:WaitForChild("HumanoidRootPart")

local flying = false
local moveInput = {
	W = false,
	A = false,
	S = false,
	D = false
}
local speed = 40
local bodyVelocity

local function onCharacterAdded(char)
	character = char
	humanoid = char:WaitForChild("Humanoid")
	hrp = char:WaitForChild("HumanoidRootPart")

	if flying then
		-- Stop flying on death, or you can comment this out if you want fly to continue after respawn
		flying = false
		if bodyVelocity then
			bodyVelocity:Destroy()
			bodyVelocity = nil
		end
		humanoid.PlatformStand = false
	end
end

player.CharacterAdded:Connect(onCharacterAdded)

local function setFlying(state)
	flying = state
	if flying then
		humanoid.PlatformStand = true
		bodyVelocity = Instance.new("BodyVelocity")
		bodyVelocity.MaxForce = Vector3.new(1e5, 1e5, 1e5)
		bodyVelocity.Velocity = Vector3.new(0, 0, 0)
		bodyVelocity.Parent = hrp
	else
		humanoid.PlatformStand = false
		if bodyVelocity then
			bodyVelocity:Destroy()
			bodyVelocity = nil
		end
	end
end

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local LocalPlayer = Players.LocalPlayer

Tabs.Main:AddToggle("Trashy Tweeen", {
	Title = "Tween",
	Default = false,
	Description = "Buns"
}):OnChanged(function(state)
	setFlying(state)
end)

UserInputService.InputBegan:Connect(function(input, gpe)
	if gpe then return end
	local key = input.KeyCode.Name
	if moveInput[key] ~= nil then
		moveInput[key] = true
	end
end)

UserInputService.InputEnded:Connect(function(input)
	local key = input.KeyCode.Name
	if moveInput[key] ~= nil then
		moveInput[key] = false
	end
end)

RunService.RenderStepped:Connect(function()
	if not flying or not bodyVelocity then return end

	local cam = workspace.CurrentCamera
	local moveDirection = Vector3.new(0, 0, 0)

	if moveInput.W then
		moveDirection += cam.CFrame.LookVector
	end
	if moveInput.S then
		moveDirection -= cam.CFrame.LookVector
	end
	if moveInput.A then
		moveDirection -= cam.CFrame.RightVector
	end
	if moveInput.D then
		moveDirection += cam.CFrame.RightVector
	end

	if moveDirection.Magnitude > 0 then
		moveDirection = moveDirection.Unit
		bodyVelocity.Velocity = moveDirection * speed

		local lookCFrame = CFrame.new(hrp.Position, hrp.Position + moveDirection)
		hrp.CFrame = hrp.CFrame:Lerp(lookCFrame, 0.2)
	else
		bodyVelocity.Velocity = Vector3.new(0, 0, 0)
	end
end)



